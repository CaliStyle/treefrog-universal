  "scripts": {
    "webpack": "./node_modules/.bin/webpack",
    "webpack-dev-server": "./node_modules/.bin/webpack-dev-server",
    "build": "webpack",
    "build:dll": "webpack --config=webpack/webpack.dll.js",
    "build:dev": "webpack --config=webpack/webpack.dev.js",
    "watch": "npm run build --watch --colors --display-error-details --display-cached",
    "server:dev:hmr": "npm run server:dev -- --inline --hot",
    "server:dev": "export NODE_ENV=development && webpack-dev-server --config=webpack/webpack.dev.js --progress --profile --watch --content-base app/",
    "server:staging": "export NODE_ENV=staging HOST=localhost PORT=3001 && npm run build && node ./dist/server/express-bundle.js",
    "server:prod": "export NODE_ENV=production && npm run build && node ./dist/server/express-bundle.js",
    "start": "node ./dist/server/express-bundle.js",
    "test": "eslint --fix . && NODE_ENV=testing && npm run build && mocha ",
    "mocha": "NODE_ENV=testing mocha",
    "tslint": "tslint -c tslint.json **/*.ts",
    "postinstall": "npm run build"
  },
  "engines": {
    "node": ">= 6.0.0",
    "npm": ">= 3"
